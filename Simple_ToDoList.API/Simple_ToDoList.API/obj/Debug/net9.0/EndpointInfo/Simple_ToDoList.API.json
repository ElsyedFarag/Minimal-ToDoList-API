{
  "openapi": "3.0.4",
  "info": {
    "title": "Simple_ToDoList.API",
    "version": "1.0"
  },
  "paths": {
    "/api/ToDos/GetToDos": {
      "get": {
        "tags": [
          "ToDos"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/ToDos/AddToDo": {
      "post": {
        "tags": [
          "ToDos"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ToDo"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ToDo"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ToDo"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ToDo": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "maxLength": 5,
            "type": "string",
            "nullable": true
          },
          "isCompleted": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
    }
  }
}